function option_picked() {
    COLOR='\033[01;31m' # bold red
    RESET='\033[00;00m' # normal white
    MESSAGE=${@:-"${RESET}Error: No message passed"}
    echo -e "${COLOR}${MESSAGE}${RESET}"
}

clear
show_menu
}


KEY1()
{
su -c am start https://shrinke.me/Sulphur &> /dev/null
sleep 1
echo -e "\033[36mEnter The Ticket Here:"
read username
if [[ $username == "DAKSLEN98" ]]
then
AG
else
echo "PLEASE VISIT LINK AND COPY TICKET CORRECTLY"
exit
fi
}


