then
AG
else
echo "PLEASE VISIT LINK AND COPY TICKET CORRECTLY"
exit
fi
}


clear
echo ""
echo -e "\033[36mHow to Get Free Ticket :-\e[m"
echo ""
echo -e "\033[35m1. We Are Opening A Link"
echo -e "\033[35m2. Copy ticket From There"
echo -e "\033[35m3. Paste Below and enter"
echo ""
sleep 9
Adarsh="$(date +"%S")"
if [[ $Adarsh > 50 ]]
then
KEY1
elif [[ $Adarsh > 30 ]]
then
KEY2
elif  [[ $Adarsh > 1 ]]
then
KEY3
else
exit
fi

