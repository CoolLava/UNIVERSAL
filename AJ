mv /data/app/$DA/lib/arm/{libTDataMaster.so,$RANDOM} &> /dev/null
touch /data/app/$DA/lib/arm/libTDataMaster.so &> /dev/null

cp /data/app/$DA/lib/arm/libtersafe.so /storage/emulated/0/Android/ &> /dev/null
mv /data/app/$DA/lib/arm/{libtersafe.so,$RANDOM} &> /dev/null
mv /storage/emulated/0/Android/libtersafe.so /data/app/$DA/lib/arm/ &> /dev/null
chmod 755 /data/app/$DA/lib/arm/libtersafe.so &> /dev/null

sleep 16
mv /data/app/$DA/lib/arm/{libgamemaster.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libcubehawk.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libtgpa.so,$RANDOM} &> /dev/null
while true
do
if [ ! $(pidof "$GAME") ]; then
su -c killall -q $GAME  &> /dev/null
rm -rf /data/media/0/Android/data/$GAME/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/*json /data/data/Sulphur &> /dev/null
 pm install -r /data/app/$GAME*/base.apk /data/app/$GAME*/base.apk  &> /dev/null
clear
echo ""
echo -e "\e[35mAntiban Closed "
exit
else
sleep 1
fi
done
}

