32BIT() {
su -c am start -n com.tencent.ig/com.epicgames.ue4.SplashActivity &> /dev/null
sleep 5
mv /data/app/$DA/lib/arm/{libzlib.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libIMSDK.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libgcloud.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libgcloudcore.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libGCloudVoice.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libtprt.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libUE4.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libijkffmpeg.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libgnustl_shared.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libPandoraVideo.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libswappy.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libvlink.so,$RANDOM} &> /dev/null
#CrashLibs
mv /data/app/$DA/lib/arm/{libTDataMaster.so,$RANDOM} &> /dev/null
touch /data/app/$DA/lib/arm/libTDataMaster.so &> /dev/null

cp /data/app/$DA/lib/arm/libtersafe.so /storage/emulated/0/Android/ &> /dev/null
mv /data/app/$DA/lib/arm/{libtersafe.so,$RANDOM} &> /dev/null
mv /storage/emulated/0/Android/libtersafe.so /data/app/$DA/lib/arm/ &> /dev/null
chmod 755 /data/app/$DA/lib/arm/libtersafe.so &> /dev/null

sleep 16
mv /data/app/$DA/lib/arm/{libgamemaster.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libcubehawk.so,$RANDOM} &> /dev/null
mv /data/app/$DA/lib/arm/{libtgpa.so,$RANDOM} &> /dev/null
while true
do
if [ ! $(pidof "$GAME") ]; then
su -c killall -q $GAME  &> /dev/null
rm -rf /data/media/0/Android/data/$GAME/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/*json /data/data/Sulphur &> /dev/null
 pm install -r /data/app/$GAME*/base.apk /data/app/$GAME*/base.apk  &> /dev/null
clear
echo ""
echo -e "\e[35mAntiban Closed "
exit
else
sleep 1
fi
done
}

